{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": 0,
    "arrow-body-style": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "object-curly-newline": 0
  }
}
